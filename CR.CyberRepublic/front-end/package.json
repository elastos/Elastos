    "build:dev": "rimraf dist && NODE_ENV=dev webpack --mode development --config ./webpack/webpack.prod.js --colors --progress",
    "lint": "eslint ./src/**/**.js",
    "test": "NODE_ENV=development jest --config jest/jest.conf.js --coverage"
  },
